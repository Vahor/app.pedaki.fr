{
  "name": "@pedaki/esbuild-plugin-tsconfig-paths",
  "version": "0.0.0",
  "private": true,
  "publishConfig": {
    "directory": "dist"
  },
  "contributors": [
    {
      "name": "wujf1@foxmail.com"
    }
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "format": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@pedaki/prettier-config"
}
